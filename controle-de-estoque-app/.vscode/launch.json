    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "chrome",
            "url": "http://localhost:8092",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "node",
            "program": "${workspaceFolder}/server.js"
        }
    ],
    "compounds": [
        {
            "name": "Server/Client",
            "configurations": ["node", "chrome"]
        }
    ]
}